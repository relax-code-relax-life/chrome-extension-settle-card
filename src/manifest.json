{
  "name": "工作居住证系统兼容助手",
  "description": "使「北京市工作居住证管理系统」兼容Chrome浏览器。",
  "default_locale": "zh",
  "version": "0.0.0.1",
  "homepage_url": "http://wangwl.net",
  "author": "wangwl",
  "icons": {
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "default_title": "工作居住证系统兼容助手",
  "manifest_version": 3,
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://219.232.200.39/uamsso/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "rules1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://219.232.200.39/uamsso/*"
      ],
      "js": [
        "loginStyle.js"
      ]
    },
    {
      "matches": [
        "*://219.232.200.42/yjrc/person/*"
      ],
      "css": [
        "applyListAction.css"
      ]
    }
  ]
}